[include]
.*/src/.*

[ignore]
.*/node_modules/.*
.*/__tests__/.*
.*/coverage/.*


[libs]
flow-typed

[options]
module.file_ext=.scss
module.file_ext=.js
module.file_ext=.svg
module.file_ext=.jsp
